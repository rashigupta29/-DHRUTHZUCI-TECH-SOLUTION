    "def Single( ar, n):\n",
    "     \n",
    "    res = ar[0]\n",
    "    for i in range(1,n):\n",
    "        res = res ^ ar[i]\n",
    "     \n",
    "    return res\n",
    " \n",
    "ar = [2, 2, 1]\n",
    "print(Single(ar, len(ar)))\n",
    " "
